    "@effect/platform": "^0.93.6",
    "@effect/platform-node": "^0.103.0",
    "ai": "6.0.0-beta.136",
    "alpinejs": "^3.15.2",
    "diff-dom": "^5.2.1",
    "effect": "^3.19.9",
    "happy-dom": "^20.0.11",
    "zod": "^4.1.13",
    "tsdown": "^0.17.0"
  }
