{
  "name": "generative-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm dev:webpage & pnpm dev:backend",
    "build": "pnpm build:webpage & pnpm build:backend",
    "preview": "pnpm preview:webpage & pnpm preview:backend",
    "dev:webpage": "pnpm --filter @betalyra/generative-ui-webpage dev",
    "build:webpage": "pnpm --filter @betalyra/generative-ui-webpage build",
    "preview:webpage": "pnpm --filter @betalyra/generative-ui-webpage preview",
    "dev:backend": "pnpm --filter @betalyra/generative-ui-backend dev",
    "test:backend": "pnpm --filter @betalyra/generative-ui-backend test",
    "build:backend": "pnpm --filter @betalyra/generative-ui-backend build",
    "dev:common": "pnpm --filter @betalyra/generative-ui-common dev",
    "build:common": "pnpm --filter @betalyra/generative-ui-common build",
    "preview:common": "pnpm --filter @betalyra/generative-ui-common preview",
    "dev:vdom-demo": "tsx scripts/vdom-demo.ts",
    "typecheck": "pnpm -r typecheck",
    "typecheck:webpage": "pnpm --filter @betalyra/generative-ui-webpage typecheck",
    "typecheck:backend": "pnpm --filter @betalyra/generative-ui-backend typecheck",
    "typecheck:common": "pnpm --filter @betalyra/generative-ui-common typecheck",
    "bench": "tsx scripts/bench.ts"
  },
  "devDependencies": {
    "@effect/vitest": "^0.27.0",
    "@types/alpinejs": "^3.13.11",
    "@types/node": "^25.0.8",
    "tinybench": "^6.0.0",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vite": "7.3.1",
    "vitest": "^4.0.17"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    },
    "onlyBuiltDependencies": [
      "onnxruntime-node"
    ]
  },
  "dependencies": {
    "@ai-sdk/google": "3.0.13",
    "@ai-sdk/groq": "3.0.15",
    "@ai-sdk/provider": "3.0.5",
    "@ai-sdk/provider-utils": "^4.0.9",
    "@effect/platform": "^0.94.2",
    "@effect/platform-node": "^0.104.1",
    "@paralleldrive/cuid2": "^3.0.6",
    "ai": "6.0.49",
    "effect": "^3.19.15",
    "happy-dom": "^20.3.7",
    "tsdown": "^0.20.1",
    "zod": "^4.3.6"
  }
}
